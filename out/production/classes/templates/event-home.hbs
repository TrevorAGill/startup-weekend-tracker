{{#partial "content"}}


    <ul class="nav nav-tabs">
        <li role="presentation" class="active"><a href="#">Home</a></li>
        <li role="presentation"><a href="/item/new">Add New Event</a></li>
    </ul>
    <div class="container">
        <!--begin main template-->
        <div class="page-header">
            <h1>All Events</h1>
        </div>
        <div class="well">
            {{#if events}}
                {{#each events}}
                    <a href="/event/{{ id }}">{{ eventName }}</a>
                {{/each}}
            {{else}}
                <p>There are no events at this time.</p>
            {{/if}}
        </div>
    </div>


{{/partial}}
{{> layout.hbs}}